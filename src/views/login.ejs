                                <!-- <span class="error" id="err-name">please enter password</span> -->
                            </div>
                            <div class="col-sm-12 contact-form-item">
                                <button type="submit" class="send_message col-sm-12 btn btn-main btn-theme wow fadeInUp" id="send" data-lang="en">Masuk</button>                
                            </div>
                            <center><a href="/register">DAFTAR</a></center>
                            <div class="clear"></div>   
                            <div class="error text-align-center" id="err-form">There was a problem validating the form please check!</div>
                            <div class="error text-align-center" id="err-timedout">The connection to the server timed out!</div>
                            <div class="error" id="err-state"></div>
                        </form> 
                                    
                        <div class="clearfix"></div>
                        <div id="ajaxsuccess">Successfully sent!!</div>
                        <div class="clear"></div>

