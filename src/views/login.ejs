                        <center><img src="/frontend/img/logo-halaka.png" class=" wow fadeInDown p-bottom-30 text-center" data-wow-duration="1s" data-wow-delay="0.6s" style="width: 75%" /></center>
                        <%- include('./partials/message.ejs') %> 
